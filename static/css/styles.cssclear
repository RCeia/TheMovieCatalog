@charset "UTF-8";
/* Variables */
/* Custom Alert Styles */
.custom-alert, .register-container .alert,
.login-container .alert {
  position: absolute;
  top: 80px; /* 70px lower plus a little extra space */
  width: 100%; /* Full width */
  left: 0;
  z-index: 1050; /* Ensure itâ€™s above other content */
}

/* General Styles */
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  font-family: "Poppins", sans-serif;
}

body {
  background: url("Wallpaper.png") no-repeat center center fixed;
  background-size: cover;
  overflow-x: hidden; /* Prevent horizontal overflow */
  color: #fff;
}

.wrapper {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
  background: rgba(39, 39, 39, 0.6);
}

/* Navbar Styles */
.nav {
  position: fixed;
  top: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 70px;
  padding: 0 20px;
  background: rgba(39, 39, 39, 0.6);
  z-index: 100;
}
.nav-logo {
  flex-shrink: 0;
  color: #3F88C5;
  font-size: 25px;
  font-weight: 1000;
  text-decoration: none; /* Ensures no underline */
  margin-right: 70px;
  margin-top: 8px;
}
.nav-logo-link {
  text-decoration: none;
}
.nav-logo-link:hover {
  text-decoration: none;
}
.nav-menu {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.nav-menu ul {
  display: flex;
  margin-top: 15px;
}
.nav-menu ul li {
  list-style-type: none;
}
.nav-menu ul li .link {
  text-decoration: none; /* Ensure links are not underlined */
  font-weight: 500;
  color: #fff;
  padding-bottom: 15px;
  margin: 15px;
}
.nav-menu ul li .link:hover, .nav-menu ul li .link.active {
  transition: color 0.3s;
  border-bottom: 2px solid #3F88C5;
  color: #3F88C5;
}
.nav-menu .nav-button {
  display: flex;
  align-items: center;
  margin-left: auto;
  /* Increase icon size and ensure proper styling */
  /* Optional: Add hover effect for the profile icon */
}
.nav-menu .nav-button i.fas.fa-user-circle {
  font-size: 30px;
  color: #fff;
  margin-left: 15px;
}
.nav-menu .nav-button i.fas.fa-user-circle:hover {
  color: #3F88C5;
  transition: 0.3s ease;
}
.nav-menu .nav-button .btn {
  position: relative;
  bottom: 0;
  width: 100px;
  font-weight: 500;
  background: #3F88C5;
  border: none;
  border-radius: 30px;
  cursor: pointer;
  transition: background 0.3s ease;
  margin-left: 10px;
}
.nav-menu .nav-button .btn:hover {
  background: #1462A2;
}
.nav-menu .nav-button .btn.white-btn {
  background: rgba(255, 255, 255, 0.7);
}
.nav-menu .nav-button .btn.white-btn:hover {
  background: #1462A2;
}
@media (max-width: 830px) {
  .nav-menu {
    flex-direction: column;
    align-items: center;
    background: rgba(0, 0, 0, 0.9);
    position: absolute;
    top: 70px;
    left: 0;
    right: 0;
    display: none;
  }
  .nav-menu ul {
    flex-direction: column;
    width: 100%;
    align-items: center; /* Center items horizontally */
    padding: 0; /* Remove default padding */
  }
  .nav-menu ul li {
    width: 100%;
    text-align: center; /* Center text inside list items */
  }
  .nav-menu ul li .link {
    padding: 15px 25px; /* Increase padding for better touch targets */
    border-bottom: 1px solid #444;
    display: block; /* Ensure links take up the full width */
    margin: 0px;
  }
  .nav-menu i.fas.fa-user-circle {
    margin-left: 0; /* Override the margin-left to 0 on small screens */
  }
  .nav-menu .nav-button {
    flex-direction: column;
    width: 100%;
    margin-top: 10px;
  }
  .nav-menu .nav-button i.fas.fa-user-circle {
    margin-left: 0 !important; /* Add !important to force override */
  }
  .nav-menu .nav-button a {
    display: flex;
    align-items: center;
  }
  .nav-menu .nav-button .btn {
    width: 90%;
    margin: 8px;
    padding: 10px 40px;
  }
  .nav-menu.responsive {
    display: flex;
  }
}
.nav-menu-btn {
  display: none;
}
@media (max-width: 830px) {
  .nav-menu-btn {
    display: block;
    margin-left: auto;
    margin-right: 10px;
    width: 30px;
    height: 30px;
    background-color: #3F88C5;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
    transition: background-color 0.3s ease;
    /* Icon within the button */
    /* Change background on hover */
  }
  .nav-menu-btn i {
    color: #fff;
    font-size: 16px; /* Adjust the size of the icon */
  }
  .nav-menu-btn:hover {
    background-color: #1462A2; /* Darker shade of the secondary color */
  }
}

/* Main Content Styles */
.main-content {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  text-align: center;
  padding: 20px;
  margin-top: 70px;
  /* Search Bar Styles */
}
.main-content .site-title {
  font-size: 64px; /* Larger font size */
  margin-bottom: 10px;
}
.main-content span.sec-color {
  color: #3F88C5;
}
.main-content .search-container {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  max-width: 950px;
  margin: 30px auto;
  margin-bottom: 100px;
}
.main-content .search-container #cover {
  width: 100%;
  padding: 5px;
  background-color: rgba(39, 39, 39, 0.8);
  border-radius: 10px;
  box-shadow: 0 10px 40px rgba(0, 0, 0, 0.5), 0 0 0 20px rgba(255, 255, 255, 0.1);
}
.main-content .search-container .tb {
  display: flex;
  width: 100%;
}
.main-content .search-container .td {
  flex: 1;
  display: flex;
}
.main-content .search-container input[type=text],
.main-content .search-container button {
  font-family: "Poppins", sans-serif;
  padding: 0;
  margin: 0;
  border: 0;
  background-color: transparent;
  color: #fff;
}
.main-content .search-container input[type=text] {
  width: calc(100% - 60px); /* Full width minus button */
  padding: 10px;
  font-size: 18px;
  background-color: transparent; /* Transparent background */
}
.main-content .search-container input[type=text]::placeholder {
  color: #bbb;
}
.main-content .search-container #s-cover {
  display: flex;
  align-items: center;
}
.main-content .search-container button {
  width: 50px;
  height: 50px;
  cursor: pointer;
  position: relative;
  border-radius: 50%;
  background-color: rgba(68, 68, 68, 0.8);
  display: flex;
  justify-content: center;
  align-items: center;
  transition: background-color 0.3s;
  margin-left: 10px;
}
.main-content .search-container button:hover {
  background-color: rgba(85, 85, 85, 0.8);
}
.main-content .search-container button #s-circle {
  width: 18px;
  height: 18px;
  border: 4px solid #3F88C5;
  border-radius: 50%;
  transition: width 0.5s, height 0.5s, border-width 0.5s, border-radius 0.5s, left 0.5s, top 0.5s;
  position: absolute;
  left: 12px;
  top: 12px;
}
.main-content .search-container button span,
.main-content .search-container button .other-line {
  position: absolute;
  width: 15px;
  height: 3px;
  background-color: #3F88C5;
  border-radius: 3px;
  transform: rotateZ(45deg);
  transition: transform 0.5s, right 0.5s, top 0.5s;
  right: 11px;
  top: 29px;
}
.main-content .search-container button:hover #s-circle {
  background-color: #3F88C5;
  width: 22px;
  height: 3px;
  border-width: 0;
  border-radius: 3px;
  left: 12px;
  top: 23px;
}
.main-content .search-container button:hover span {
  width: 12.5px;
  transform: rotate(45deg);
  right: 12px;
  top: 19px;
}
.main-content .search-container button:hover .other-line {
  width: 12.5px;
  transform: rotate(-45deg);
  right: 12px;
  top: 27px;
}

/* Footer Styles */
.footer {
  background: rgba(39, 39, 39, 0.6);
  color: #fff;
  text-align: center;
  padding: 15px;
  justify-content: center;
}
.footer p {
  margin-bottom: 0;
}

/* Movie Slide Styling */
/* Container for Movies Section */
.movies-container {
  width: 80%;
  margin: 0 auto;
  padding-top: 20px;
  justify-content: center;
  /* Section Title */
  /* Carousel */
}
.movies-container .section-title {
  font-size: 2em;
  color: white;
  text-align: left;
  margin-bottom: 20px;
  margin-left: 15px; /* Space below the title */
}
.movies-container .movie-carousel {
  margin-bottom: 40px; /* Adjusts the spacing below the carousel */
  /* Movie Slide */
}
.movies-container .movie-carousel .movie-slide {
  display: grid;
  position: relative;
  text-align: center;
  padding: 10px;
  color: white; /* Ensures text is white */
  overflow: hidden; /* Ensure icons are not visible outside the slide */
  border-radius: 10px;
  justify-items: center;
  /* Image Styles */
}
.movies-container .movie-carousel .movie-slide .movie-icons {
  bottom: 58px;
}
.movies-container .movie-carousel .movie-slide:hover .movie-icons {
  opacity: 1; /* Show icons on hover */
  visibility: visible; /* Ensure icons are visible on hover */
}
.movies-container .movie-carousel .movie-slide img {
  width: 100%;
  max-width: 220px;
  height: auto;
  border-radius: 10px;
  transition: filter 0.3s ease; /* Smooth transition for darkening effect */
}
.movies-container .movie-carousel .movie-slide h3 {
  margin-top: 10px;
  font-size: 1.2em;
  text-align: center;
}
.movies-container .movie-carousel .movie-slide:hover img {
  filter: brightness(50%); /* Darkens the image */
}

/* Section Title Styling */
.section-title {
  font-size: 2em;
  color: white;
  text-align: left;
  margin-bottom: 20px;
  margin-left: 15px; /* Space below the title */
}

/* Register Page Styles */
.register-container,
.login-container {
  width: 100%;
  max-width: 600px;
  margin: 0 auto;
  padding: 20px;
  background-color: rgba(39, 39, 39, 0.8);
  border-radius: 10px;
  box-shadow: 0 10px 40px rgba(0, 0, 0, 0.5);
}
.register-container h1,
.login-container h1 {
  font-size: 36px;
  margin-bottom: 20px;
}
.register-container h1 .sec-color,
.login-container h1 .sec-color {
  color: #3F88C5;
}
.register-container .form-group,
.login-container .form-group {
  margin-bottom: 15px;
}
.register-container .form-group label,
.login-container .form-group label {
  display: block;
  margin-bottom: 5px;
  color: #fff;
}
.register-container .form-group input[type=text],
.register-container .form-group input[type=password],
.register-container .form-group input[type=email],
.login-container .form-group input[type=text],
.login-container .form-group input[type=password],
.login-container .form-group input[type=email] {
  width: 100%;
  padding: 10px;
  border: 1px solid #ccc;
  border-radius: 5px;
  background-color: #fff;
  color: #333;
}
.register-container .form-group button,
.login-container .form-group button {
  width: 100%;
  padding: 10px;
  background-color: #3F88C5;
  border: none;
  border-radius: 5px;
  color: #fff;
  font-size: 16px;
  cursor: pointer;
  transition: background-color 0.3s;
}
.register-container .form-group button:hover,
.login-container .form-group button:hover {
  background-color: #1462A2;
}
.register-container .checkbox-group,
.login-container .checkbox-group {
  display: flex;
  justify-content: center;
}
.register-container .checkbox-group input[type=checkbox],
.login-container .checkbox-group input[type=checkbox] {
  margin-right: 10px;
}
.register-container .checkbox-group label,
.login-container .checkbox-group label {
  margin-bottom: 0;
}
.register-container .checkbox-group label a,
.login-container .checkbox-group label a {
  color: #3F88C5;
  text-decoration: none;
}
.register-container .checkbox-group label a:hover,
.login-container .checkbox-group label a:hover {
  text-decoration: underline;
}
/* Movie Icons Container */
.movie-icons {
  position: absolute;
  bottom: 30px; /* Place icons slightly above the bottom */
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: center; /* Center icons horizontally */
  gap: 10px;
  opacity: 0; /* Hidden by default */
  visibility: hidden; /* Hidden by default */
  transition: opacity 0.3s ease, visibility 0.3s ease; /* Smooth transition for visibility */
  padding: 10px; /* Space from the bottom */
  width: calc(100% - 40px); /* Full width with padding */
  box-sizing: border-box;
  z-index: 10; /* Ensure icons are above the image */
  width: 100%;
  /* Show icons only when hovering over the movie-poster */
}
.movie-icons button {
  background: rgba(0, 0, 0, 0.6); /* Slightly darker background for icons */
  border: none;
  border-radius: 50%;
  width: 40px;
  height: 40px;
  color: #fff;
  font-size: 18px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: background 0.3s ease;
}
.movie-icons button:hover {
  background: rgba(0, 0, 0, 0.8); /* Darker background on hover */
}
.movie-icons button i {
  font-size: 20px;
}

.movie-container {
  display: flex; /* Flexbox for horizontal layout */
  width: 100%; /* Full width of the page */
  gap: 20px; /* Space between the two columns */
  margin-bottom: 5px;
  padding-left: 4%;
  color: white; /* Ensures text is white */
}
.movie-container .movie-poster-container {
  justify-content: center;
  width: 35%; /* Set the width to 35% of the screen */
  max-width: 300px; /* Maximum width to ensure it doesn't get too large */
  min-width: 200px; /* Minimum width to ensure it doesn't get too small */
  position: relative;
  overflow: hidden; /* Ensure icons and images are not visible outside the container */
  margin: 20px;
}
.movie-container .movie-poster-wrapper {
  width: 100%; /* Full width within the poster container */
  position: relative; /* Position relative to contain the absolutely positioned icons */
  border-radius: 10px; /* Rounded corners for the wrapper */
  overflow: hidden; /* Hide overflow to keep icons within the bounds */
  /* Hover effect only on the movie-poster */
}
.movie-container .movie-poster-wrapper .movie-poster {
  width: 100%; /* Maintain full width within the container */
  height: auto; /* Maintain the aspect ratio of the image */
  border-radius: 10px; /* Rounded corners for the poster */
  transition: filter 0.3s ease; /* Smooth transition for darkening effect */
  align-self: center;
  margin: 20px 0;
}
.movie-container .movie-poster-wrapper:hover .movie-poster {
  filter: brightness(50%); /* Darkens the image */
}
.movie-container .movie-poster-wrapper:hover .movie-icons {
  opacity: 1; /* Show icons on hover */
  visibility: visible; /* Ensure icons are visible on hover */
}
.movie-container .movie-details {
  width: 75%; /* Take up 65% of the screen width */
  display: flex;
  flex-direction: column; /* Stack movie details vertically */
  gap: 15px; /* Space between movie details */
  text-align: left;
  padding-left: 4%;
}
.movie-container .movie-details h2 {
  text-align: left;
  margin-left: 11%;
}
@media (max-width: 1024px) {
  .movie-container {
    flex-direction: column; /* Stack elements vertically on smaller screens */
    align-items: center;
    max-width: 600px;
    width: 100%;
  }
  .movie-container .movie-poster-container {
    max-width: 400px;
    width: 100%; /* Take up full width on smaller screens */
    margin-bottom: 20px;
    align-items: center;
  }
  .movie-container .movie-details {
    width: 100%; /* Take up full width on smaller screens */
    text-align: center;
  }
}

/* Profile Page Styles */
.profile-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  padding: 20px;
  color: #fff;
}
.profile-container .profile-header {
  display: flex;
  align-items: center;
  justify-content: space-between; /* Aligns left, center, and right sections */
  margin-bottom: 20px;
  width: 100%;
  max-width: 1200px;
  padding: 10px;
  background: rgba(39, 39, 39, 0.8);
  border-radius: 10px;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);
  position: relative; /* Allows for absolute positioning */
  /* Left: Profile Icon */
  /* Center: Profile Info */
  /* Right: Follow/Unfollow Button */
}
.profile-container .profile-header .left-side {
  flex-shrink: 0;
  display: flex;
  align-items: center;
  margin-right: auto; /* Ensures it's pushed to the left */
}
.profile-container .profile-header .left-side .profile-icon {
  width: 100px;
  height: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.profile-container .profile-header .left-side .profile-icon .fas.fa-user-circle {
  font-size: 85px; /* Match the size to fit */
  color: #fff;
}
.profile-container .profile-header .profile-info {
  display: flex;
  flex-direction: column; /* Aligns username and links vertically */
  align-items: center; /* Centers content */
  text-align: center; /* Centers text */
  flex: 1; /* Takes remaining space between left and right */
}
.profile-container .profile-header .profile-info .profile-name {
  font-size: 2em;
  margin-bottom: 10px;
}
.profile-container .profile-header .profile-info .profile-links {
  display: flex; /* Align links in a row */
  justify-content: center; /* Center the links */
}
.profile-container .profile-header .profile-info .profile-links a {
  color: #fff;
  text-decoration: none;
  font-weight: 500;
  margin-right: 20px;
}
.profile-container .profile-header .profile-info .profile-links a:hover {
  color: #3F88C5;
  border-bottom: 2px solid #3F88C5;
  transition: color 0.3s, border-bottom 0.3s;
}
.profile-container .profile-header .profile-info .profile-links a .count {
  font-weight: bold;
  margin-left: 5px;
}
.profile-container .profile-header .right-side {
  flex-shrink: 0;
  display: flex;
  align-items: center;
  margin-left: auto; /* Ensures it's pushed to the right */
}
.profile-container .profile-header .right-side .follow-buttons {
  display: flex;
  gap: 20px;
  margin-right: 20px;
}
.profile-container .profile-header .right-side .follow-buttons form {
  margin: 0;
}
.profile-container .profile-header .right-side .follow-buttons form button {
  font-size: 16px;
  font-weight: 500;
  padding: 10px 20px;
  border-radius: 5px;
  cursor: pointer;
  transition: background-color 0.3s, color 0.3s;
}
.profile-container .profile-header .right-side .follow-buttons form button.btn-primary {
  background-color: #3F88C5;
  border: none;
  color: #fff;
}
.profile-container .profile-header .right-side .follow-buttons form button.btn-primary:hover {
  background-color: #1462A2;
}
.profile-container .profile-header .right-side .follow-buttons form button.btn-warning {
  background-color: #ffad00;
  border: none;
  color: #fff;
}
.profile-container .profile-header .right-side .follow-buttons form button.btn-warning:hover {
  background-color: #e59400;
}

/* User List Page Styles */
.user-list-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  padding: 20px;
  color: #fff;
}
.user-list-container .section-title {
  font-size: 2em;
  margin-bottom: 20px;
  margin-left: 0px;
  color: #fff;
}
.user-list-container .user-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  gap: 30px; /* Gap between each user card */
  width: 100%;
  max-width: 1200px;
}
.user-list-container .user-list .user-card {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: rgba(39, 39, 39, 0.8);
  padding: 20px;
  border-radius: 10px;
  width: 150px;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);
  text-align: center;
  transition: transform 0.3s;
}
.user-list-container .user-list .user-card:hover {
  transform: translateY(-5px);
}
.user-list-container .user-list .user-card .user-icon {
  font-size: 80px; /* Icon size */
  color: #fff;
  margin-bottom: 10px;
}
.user-list-container .user-list .user-card .user-name {
  font-size: 1.2em;
  color: #fff;
}

/* Search Results Page Styles */
.search-results-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  padding: 20px;
  color: #fff;
}
.search-results-container .section-title {
  font-size: 2em;
  margin-bottom: 20px;
  margin-left: 0;
  color: #fff;
}
.search-results-container .movie-results {
  display: grid;
  grid-template-columns: repeat(6, 1fr); /* 6 movies per row */
  gap: 20px; /* Gap between each movie card */
  width: 100%;
  max-width: 1200px;
}
.search-results-container .movie-results .movie-card {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: rgba(39, 39, 39, 0.8);
  padding: 15px;
  border-radius: 10px;
  width: 180px;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);
  text-align: center;
  transition: transform 0.3s, background 0.3s;
}
.search-results-container .movie-results .movie-card:hover {
  transform: translateY(-5px);
  background: rgba(20, 20, 20, 0.9); /* Darker background on hover */
}
.search-results-container .movie-results .movie-card .movie-poster {
  width: 100%;
  border-radius: 10px;
  margin-bottom: 10px;
}
.search-results-container .movie-results .movie-card .no-poster {
  width: 100%;
  height: 270px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(100, 100, 100, 0.5);
  color: #fff;
  border-radius: 10px;
  margin-bottom: 10px;
}
.search-results-container .movie-results .movie-card .movie-info .movie-title {
  font-size: 1.2em;
  color: #fff;
  margin-bottom: 10px;
}
.search-results-container .movie-results .movie-card .movie-info .release-date {
  font-size: 1em;
  color: #fff;
}
.search-results-container .back-home-link {
  margin-top: 30px;
  font-size: 1.2em;
  color: #fff;
  text-decoration: none;
  transition: color 0.3s;
}
.search-results-container .back-home-link:hover {
  color: white;
