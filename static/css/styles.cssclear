/* Friends Page Styles */
.friends-header {
  display: flex;
  flex-direction: column; /* Stack heading and search bar vertically */
  align-items: center; /* Center items horizontally */
  width: 100%;
  padding: 20px;
  color: #fff;
  margin-bottom: 50px; /* Space below header */
}
.friends-header .site-title {
  font-size: 2.5em;
  color: #fff;
  margin: 0; /* Remove default margin */
}
.friends-header .search-container.search-friends {
  width: 100%;
  max-width: 600px; /* Adjust width as needed */
  margin-top: 20px; /* Space between heading and search bar */
}
.friends-header .search-container.search-friends #cover {
  width: 100%;
  padding: 5px;
  background-color: rgba(39, 39, 39, 0.8);
  border-radius: 10px;
  box-shadow: 0 10px 40px rgba(0, 0, 0, 0.5), 0 0 0 20px rgba(255, 255, 255, 0.1);
}
.friends-header .search-container.search-friends .tb {
  display: flex;
  width: 100%;
}
.friends-header .search-container.search-friends .td {
  flex: 1;
  display: flex;
}
.friends-header .search-container.search-friends input[type=text],
.friends-header .search-container.search-friends button {
  font-family: "Poppins", sans-serif;
  padding: 0;
  margin: 0;
  border: 0;
  background-color: transparent;
  color: #fff;
}
.friends-header .search-container.search-friends input[type=text] {
  width: calc(100% - 60px); /* Full width minus button */
  padding: 10px;
  font-size: 18px;
  background-color: transparent; /* Transparent background */
}
.friends-header .search-container.search-friends input[type=text]::placeholder {
  color: #bbb;
}
.friends-header .search-container.search-friends #s-cover {
  display: flex;
  align-items: center;
}
.friends-header .search-container.search-friends button {
  width: 50px;
  height: 50px;
  cursor: pointer;
  position: relative;
  border-radius: 50%;
  background-color: rgba(68, 68, 68, 0.8);
  display: flex;
  justify-content: center;
  align-items: center;
  transition: background-color 0.3s;
  margin-left: 10px;
}
.friends-header .search-container.search-friends button:hover {
  background-color: rgba(85, 85, 85, 0.8);
}
.friends-header .search-container.search-friends button #s-circle {
  width: 18px;
  height: 18px;
  border: 4px solid #3F88C5;
  border-radius: 50%;
  transition: width 0.5s, height 0.5s, border-width 0.5s, border-radius 0.5s, left 0.5s, top 0.5s;
  position: absolute;
  left: 12px;
  top: 12px;
}
.friends-header .search-container.search-friends button span,
.friends-header .search-container.search-friends button .other-line {
  position: absolute;
  width: 15px;
  height: 3px;
  background-color: #3F88C5;
  border-radius: 3px;
  transform: rotateZ(45deg);
  transition: transform 0.5s, right 0.5s, top 0.5s;
  right: 11px;
  top: 29px;
}
.friends-header .search-container.search-friends button:hover #s-circle {
  background-color: #3F88C5;
  width: 22px;
  height: 3px;
  border-width: 0;
  border-radius: 3px;
  left: 12px;
  top: 23px;
}
.friends-header .search-container.search-friends button:hover span {
  width: 12.5px;
  transform: rotate(45deg);
  right: 12px;
  top: 19px;
}
.friends-header .search-container.search-friends button:hover .other-line {
  width: 12.5px;
  transform: rotate(-45deg);
  right: 12px;
  top: 27px;
}

